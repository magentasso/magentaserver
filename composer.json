    "require": {
        "magentasso/magentasso-php": "dev-main",
        "christian-riesen/base32": "^1.6",
        "slim/slim": "4.*",
        "slim/psr7": "^1.3",
        "slim/twig-view": "^3.0",
        "php-di/php-di": "^6.3",
        "vlucas/phpdotenv": "^5.3",
        "philipp15b/php-i18n": "^4.0",
        "gilbitron/easycsrf": "^1.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.86",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MagentaServer\\": "lib/"
        }
    }
