{
    "name": "magentasso/magentaserver",
    "description": "The reference standalone Magenta server",
    "type": "project",
    "license": "AGPLv3",
    "authors": [
        {
            "name": "Iris System",
            "email": "iris@iris.ac.nz"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
